[flake8]
max-line-length = 100
extend-ignore = E203, E266, E501, W503
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info,
    .eggs,
    node_modules,
    my-research-platform,
    stock-platform-server
max-complexity = 10
per-file-ignores =
    __init__.py:F401
